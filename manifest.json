{
    "name": "Audiobookshelf",
    "id": "audiobookshelf",
    "packaging_format": 1,
    "description": {
        "en": "Audiobook server for managing and playing your audiobooks",
        "fr": "Serveur de livres audio pour gÃ©rer et lire vos livres audio"
    },
    "version": "2.2.17~ynh1",
    "url": "https://audiobookshelf.org/",
    "upstream": {
        "license": "AGPL-3.0-only",
        "website": "https://audiobookshelf.org/",
        "demo": "https://audiobookshelf.org/",
        "userdoc": "https://www.audiobookshelf.org/docs",
        "code": "https://github.com/advplyr/audiobookshelf"
    },
    "license": "AGPL-3.0-only",
    "maintainer": {
        "name": "Navan Chauhan",
        "email": "audiobookshelf_ynh@navan.email"
    },
    "requirements": {
        "yunohost": ">= 4.3.0"
    },
    "multi_instance": true,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install": [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
